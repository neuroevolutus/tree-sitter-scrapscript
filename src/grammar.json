{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "scrapscript",
  "rules": {
    "program": {
      "type": "SYMBOL",
      "name": "_subprogram"
    },
    "_subprogram": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_expr"
        },
        {
          "type": "SYMBOL",
          "name": "where"
        }
      ]
    },
    "where": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_subprogram"
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "SYMBOL",
          "name": "declaration"
        }
      ]
    },
    "declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_annotation"
        },
        {
          "type": "SYMBOL",
          "name": "_binding"
        }
      ]
    },
    "_annotation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pattern"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "id"
        }
      ]
    },
    "_binding": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "pattern"
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "SYMBOL",
              "name": "_expr"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_annotation"
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "SYMBOL",
              "name": "_expr"
            }
          ]
        }
      ]
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "_line_comment": {
      "type": "PATTERN",
      "value": "--.*"
    },
    "_expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "apply"
        },
        {
          "type": "SYMBOL",
          "name": "bytes"
        },
        {
          "type": "SYMBOL",
          "name": "fun"
        },
        {
          "type": "SYMBOL",
          "name": "hole"
        },
        {
          "type": "SYMBOL",
          "name": "id"
        },
        {
          "type": "SYMBOL",
          "name": "infix"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "match_fun"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "parens"
        },
        {
          "type": "SYMBOL",
          "name": "record"
        },
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "parens": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "expr",
          "content": {
            "type": "SYMBOL",
            "name": "_subprogram"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_unary": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bytes"
        },
        {
          "type": "SYMBOL",
          "name": "hole"
        },
        {
          "type": "SYMBOL",
          "name": "id"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "parens"
        },
        {
          "type": "SYMBOL",
          "name": "record"
        },
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "wildcard"
        }
      ]
    },
    "_callable": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "apply"
        },
        {
          "type": "SYMBOL",
          "name": "id"
        },
        {
          "type": "SYMBOL",
          "name": "parens"
        }
      ]
    },
    "apply": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "caller",
          "content": {
            "type": "SYMBOL",
            "name": "_callable"
          }
        },
        {
          "type": "FIELD",
          "name": "callee",
          "content": {
            "type": "SYMBOL",
            "name": "_unary"
          }
        }
      ]
    },
    "infix": {
      "type": "PREC_LEFT",
      "value": 10,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "left",
            "content": {
              "type": "SYMBOL",
              "name": "_expr"
            }
          },
          {
            "type": "FIELD",
            "name": "op",
            "content": {
              "type": "SYMBOL",
              "name": "op"
            }
          },
          {
            "type": "FIELD",
            "name": "right",
            "content": {
              "type": "SYMBOL",
              "name": "_expr"
            }
          }
        ]
      }
    },
    "number": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[0-9]+"
            },
            {
              "type": "PATTERN",
              "value": "[0-9]+\\.[0-9]+"
            }
          ]
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "bytes": {
      "type": "PATTERN",
      "value": "~~[A-Za-z0-9+/=]+"
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_expr"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_expr"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "id": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "hole": {
      "type": "STRING",
      "value": "()"
    },
    "tag": {
      "type": "PREC_RIGHT",
      "value": 5,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "SYMBOL",
            "name": "id"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_unary"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "record": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "field"
                    },
                    {
                      "type": "REPEAT",
                      "content": {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "STRING",
                            "value": ","
                          },
                          {
                            "type": "SYMBOL",
                            "name": "field"
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "field": {
      "type": "CHOICE",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "id"
          }
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "name",
              "content": {
                "type": "SYMBOL",
                "name": "id"
              }
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "_expr"
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": ".."
            },
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "_expr"
              }
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "record_wildcard",
          "content": {
            "type": "STRING",
            "value": "..."
          }
        }
      ]
    },
    "fun": {
      "type": "PREC_RIGHT",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "pattern"
          },
          {
            "type": "STRING",
            "value": "->"
          },
          {
            "type": "SYMBOL",
            "name": "_expr"
          }
        ]
      }
    },
    "match_fun": {
      "type": "PREC_RIGHT",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "|"
          },
          {
            "type": "SYMBOL",
            "name": "match_arm"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "|"
                },
                {
                  "type": "SYMBOL",
                  "name": "match_arm"
                }
              ]
            }
          }
        ]
      }
    },
    "match_arm": {
      "type": "PREC_RIGHT",
      "value": 4,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "pattern"
          },
          {
            "type": "STRING",
            "value": "->"
          },
          {
            "type": "SYMBOL",
            "name": "_expr"
          }
        ]
      }
    },
    "pattern": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bytes"
        },
        {
